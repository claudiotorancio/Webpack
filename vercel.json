{
  "routes": [
    {
      "src": "/api/books",
      "methods": ["POST"],
      "dest": "/api/BookServices.js"
    },
    {
      "src": "/api/books",
      "methods": ["GET"],
      "dest": "/api/BookServices.js"
    },
    {
      "src": "/api/books/:id",
      "methods": ["DELETE"],
      "dest": "/api/BookServices.js"
    }
  ]
}